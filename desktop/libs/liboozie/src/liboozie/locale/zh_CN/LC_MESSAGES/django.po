#: src/liboozie/conf.py:27
msgid "URL of Oozie server. This is required for job submission."
msgstr "Oozie 服务器的 URL。提交作业时需要此 URL。"

#: src/liboozie/conf.py:33
msgid "Whether Oozie requires client to perform Kerberos authentication."
msgstr "Oozie 是否需要客户端执行 Kerberos 身份验证。"

#: src/liboozie/conf.py:40
msgid ""
"Location on HDFS where the workflows/coordinators are deployed when "
"submitted by a non-owner."
msgstr "由非所有者提交时，workflow/coordinator 在 HDFS 上部署的位置。"

#: src/liboozie/conf.py:68
msgid "The Oozie server is not available"
msgstr "Oozie Server 不适用"

#: src/liboozie/conf.py:77
msgid "Oozie Share Lib not installed in default location."
msgstr "Oozie 分享库 (Oozie Share Lib) 无法安装到默认位置。"

#: src/liboozie/submittion.py:68
#, python-format
msgid "Submission already submitted (Oozie job id %s)"
msgstr "已提交提交（Oozie 作业 ID %s）"

#: src/liboozie/submittion.py:147
msgid "Failed to access deployment directory."
msgstr "无法访问部署目录。"

#: src/liboozie/submittion.py:204
#, python-format
msgid "Path is not a directory: %s."
msgstr "路径不是目录：%s。"

#: src/liboozie/submittion.py:209
#, python-format
msgid "Error accessing directory '%s': %s."
msgstr "访问目录 '%s' 时出错：%s。"

#: src/liboozie/types.py:356
msgid "Insufficient permission."
msgstr "无足够权限。"

#: src/liboozie/types.py:357
#, python-format
msgid "Permission denied. User %(username)s cannot modify user %(user)s's job."
msgstr "权限被拒绝。用户 %(username)s 无法修改用户 %(user)s 的作业。"

